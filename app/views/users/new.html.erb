<% if @user.errors.any? %>
  <div class="ui warning message">
    <div class="header">
      The form contains <%= pluralize(@user.errors.count, 'errors') %>
    </div>
    <ul class="list">
    <% @user.errors.full_massages.each do |full_message| %>
      <li><%= full_message %></li>
    <% end %>
    </ul>
  </div>
<% end %>
